<%- include('headers') %> 

		<div class="box" id="heading">
			<h1><%= listTitle %></h1>
		</div>

		<div class="box">
			<% for (let i=0; i < items.length; i++) { %>
			<div class="item">
				<input type="checkbox" />
				<p><%= items[i] %></p>
			</div>
			<% } %>
			<form action="/" class="item" method="post">
				<input
					type="text"
					name="newItem"
					placeholder="New Todo"
					autocomplete="off"
				/>
				<button type="submit" name="list" value=<%= listTitle %>>+</button>
			</form>
		</div>

<%- include('footers') %> 
